{
  "name": "emails-input",
  "version": "0.0.0",
  "description": "An easy way to input emails on your app!",
  "main": "dist/emails-input.cjs.js",
  "module": "dist/emails-input.esm.js",
  "browser": "dist/emails-input.umd.js",
  "scripts": {
    "build": "rollup -c",
    "start": "npm run build -- rollup.dev.js -w",
    "lint": "eslint . --fix",
    "test": "run-p test:*",
    "test:e2e": "run-p test:e2e:*",
    "test:e2e:chrome": "cross-env EI_BROWSER=chrome npm run dev",
    "pretest": "npm run build",
    "ghp": "npm run build && rm -fr docs && cp -R public docs && mv dist/emails-input.cjs.js docs/emails-input.umd.js",
    "dev": "jest e2e/selenium.js"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvcds/emails-input.git"
  },
  "author": "Marcos Silva <marvinc.silva@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/mvcds/emails-input/issues"
  },
  "homepage": "https://github.com/mvcds/emails-input#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "chromedriver": "^83.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "rollup": "^2.21.0",
    "rollup-plugin-inline-svg": "^1.1.1",
    "rollup-plugin-serve": "^1.0.1",
    "selenium-webdriver": "^4.0.0-alpha.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run dev"
    }
  },
  "dependencies": {
    "random-email": "^1.0.3"
  }
}
